{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "installCommand": "npm ci --legacy-peer-deps --no-optional",
  "rewrites": [
    {
      "source": "/services/api/(.*)",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/$1"
    },
    {
      "source": "/auth/unified/(.*)",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/auth/unified/$1"
    },
    {
      "source": "/auth/google/(.*)",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/auth/google/$1"
    },
    {
      "source": "/auth/login",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/auth/login"
    },
    {
      "source": "/auth/logout",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/auth/logout"
    },
    {
      "source": "/auth/refresh",
      "destination": "https://restbackend-dc8667cf0950.herokuapp.com/auth/refresh"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "https://www.organizesee.com.br/services/api",
    "VITE_API_KEY": "}$gQ7TlDEhJ88np]^n8[uFu{9f#;+8qjZ&?c[+Sj_CLhMO[Z(iM_)ZnW]j2M]+j+"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-API-Key",
          "value": "}$gQ7TlDEhJ88np]^n8[uFu{9f#;+8qjZ&?c[+Sj_CLhMO[Z(iM_)ZnW]j2M]+j+"
        }
      ]
    },
    {
      "source": "/services/api/(.*)",
      "headers": [
        {
          "key": "X-API-Key",
          "value": "}$gQ7TlDEhJ88np]^n8[uFu{9f#;+8qjZ&?c[+Sj_CLhMO[Z(iM_)ZnW]j2M]+j+"
        }
      ]
    }
  ]
}
