{
  "name": "organizesee-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Sistema de Organização Financeira Pessoal",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "start": "vite --port 3000",
    "start:build": "vite preview --port 3000",
    "start:prod": "npm run build:prod && npm run start:build",
    "build": "vite build --mode production",
    "build:prod": "vite build --config config/vite.config.production.ts --mode production",
    "build:react-simple": "set NODE_ENV=production && vite build --config vite.config.react-simple.ts --mode production",
    "build:with-typecheck": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "prod:cleanup": "node scripts/production-cleanup.cjs",
    "prod:optimize": "node scripts/production-optimize.cjs"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "finance",
    "budget",
    "organization"
  ],
  "author": "Jonathan Santos",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
