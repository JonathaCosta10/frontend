{
  "name": "organizesee-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Plataforma completa de gest√£o financeira com funcionalidades premium",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "npx vite build --config vite.config.minimal.ts",
    "build:prod": "vite build --config vite.config.prod.ts",
    "build:ultra": "vite build --config vite.config.ultra.ts",
    "build:vercel": "npx vite build --config vite.config.minimal.ts",
    "build:with-typecheck": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build:ultra",
    "build:optimized": "npm run typecheck && npm run build:ultra",
    "preview": "vite preview",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "prod:cleanup": "node scripts/production-cleanup.cjs",
    "prod:optimize": "node scripts/production-optimize.cjs",
    "prod:validate": "node scripts/production-validate.cjs",
    "prod:prepare": "npm run prod:cleanup && npm run prod:optimize && npm run prod:validate && npm run build:ultra",
    "test:refresh": "node scripts/test-refresh-token.js",
    "env": "powershell -ExecutionPolicy Bypass -File scripts/set-environment.ps1",
    "env:dev": "powershell -ExecutionPolicy Bypass -File scripts/set-environment.ps1 development",
    "env:prod": "powershell -ExecutionPolicy Bypass -File scripts/set-environment.ps1 production",
    "dev:local": "npm run env:dev && npm run dev",
    "build:prod-env": "npm run env:prod && npm run build:ultra",
    "postinstall": "npm install vite",
    "prebuild": "node vercel-prebuild.js",
    "perf:analyze": "node scripts/analyze-performance.cjs",
    "perf:build": "npm run build:ultra && npm run perf:analyze",
    "bundle:visualize": "ANALYZE=true npm run build:ultra"
  },
